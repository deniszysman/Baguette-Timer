// MARK: - Sourdough Bread Ball
"recipe.sourdough.bread.ball.step.1.instruction" = "Prepare the dough";
"recipe.sourdough.bread.ball.step.1.notes" = "‚Ä¢ 325 g water\n‚Ä¢ 10 g salt\n‚Ä¢ 100 g starter\n‚Ä¢ 450 g bread flour\n‚Ä¢ Mix until combined\n‚Ä¢ Cover with towel";
"recipe.sourdough.bread.ball.step.2.instruction" = "Stretch & Fold #1";
"recipe.sourdough.bread.ball.step.2.notes" = "‚Ä¢ Wet hands\n‚Ä¢ Lift one side of the dough and fold over\n‚Ä¢ Rotate bowl ¬º turn and repeat 3‚Äì4 times\n‚Ä¢ Cover with towel";
"recipe.sourdough.bread.ball.step.3.instruction" = "Stretch & Fold #2";
"recipe.sourdough.bread.ball.step.3.notes" = "‚Ä¢ Repeat stretch-and-fold process\n‚Ä¢ Keep dough covered";
"recipe.sourdough.bread.ball.step.4.instruction" = "Stretch & Fold #3";
"recipe.sourdough.bread.ball.step.4.notes" = "‚Ä¢ Repeat stretch-and-fold process\n‚Ä¢ Keep dough covered";
"recipe.sourdough.bread.ball.step.5.instruction" = "Stretch & Fold #4";
"recipe.sourdough.bread.ball.step.5.notes" = "‚Ä¢ Final stretch-and-fold\n‚Ä¢ Dough should feel tighter and smoother\n‚Ä¢ Cover with towel";
"recipe.sourdough.bread.ball.step.6.instruction" = "Shape the dough ball";
"recipe.sourdough.bread.ball.step.6.notes" = "‚Ä¢ Lightly flour the counter\n‚Ä¢ Shape into a tight round ball (boule)\n‚Ä¢ Place seam-side up in a floured bowl/banneton\n‚Ä¢ Cover with towel";
"recipe.sourdough.bread.ball.step.7.instruction" = "Final Proof (Cold Fermentation)";
"recipe.sourdough.bread.ball.step.7.notes" = "‚Ä¢ Place dough in refrigerator\n‚Ä¢ Proof overnight (12‚Äì16 hours)\n‚Ä¢ Dough will rise slowly and develop flavor";
"recipe.sourdough.bread.ball.step.8.instruction" = "Preheat the oven";
"recipe.sourdough.bread.ball.step.8.notes" = "‚Ä¢ Preheat oven to 475¬∞F (245¬∞C)\n‚Ä¢ Put Dutch oven inside to heat\n‚Ä¢ Allow at least 30 minutes of preheating";
"recipe.sourdough.bread.ball.step.9.instruction" = "Score & Bake";
"recipe.sourdough.bread.ball.step.9.notes" = "‚Ä¢ Remove dough from fridge\n‚Ä¢ Flip onto parchment\n‚Ä¢ Score the top\n‚Ä¢ Bake in Dutch oven:\n‚Ä¢ 20 min lid ON\n‚Ä¢ 20‚Äì25 min lid OFF\n‚Ä¢ Cool 1 hour before slicing";
"recipe.sourdough.bread.ball.step.10.instruction" = "Your bread is ready!";
"recipe.sourdough.bread.ball.step.10.notes" = "‚Ä¢ Slice and enjoy\n\nEnd of the recipe";

// MARK: - Brioche
"recipe.brioche.step.1.instruction" = "Prepare the dough (Mixing #1)";
"recipe.brioche.step.1.notes" = "‚Ä¢ 250 g milk (lukewarm)\n‚Ä¢ 10 g dry yeast\n‚Ä¢ 60 g sugar\n‚Ä¢ 3 eggs\n‚Ä¢ Mix until combined";
"recipe.brioche.step.2.instruction" = "Add flour & salt (Mixing #2)";
"recipe.brioche.step.2.notes" = "‚Ä¢ Add 500 g flour\n‚Ä¢ Add 10 g salt\n‚Ä¢ Mix until dough starts forming";
"recipe.brioche.step.3.instruction" = "Knead with butter (Mixing #3)";
"recipe.brioche.step.3.notes" = "‚Ä¢ Add 150 g softened butter, piece by piece\n‚Ä¢ Knead until dough is smooth, elastic, and shiny\n‚Ä¢ Dough will be sticky ‚Äî this is normal";
"recipe.brioche.step.4.instruction" = "First Rise (Bulk Fermentation)";
"recipe.brioche.step.4.notes" = "‚Ä¢ Cover the bowl\n‚Ä¢ Leave in warm room\n‚Ä¢ Dough should double in size";
"recipe.brioche.step.5.instruction" = "Degas the dough";
"recipe.brioche.step.5.notes" = "‚Ä¢ Gently press down to remove excess air\n‚Ä¢ Do not over-knead\n‚Ä¢ Prepare for shaping";
"recipe.brioche.step.6.instruction" = "Shape the Brioche";
"recipe.brioche.step.6.notes" = "Choose one:\n‚Ä¢ Brioche loaf ‚Üí divide into 3‚Äì4 balls, place in loaf pan\n‚Ä¢ Brioche balls ‚Üí form tight round balls, place in mold\n‚Ä¢ Cover with towel";
"recipe.brioche.step.7.instruction" = "Second Rise (Proofing)";
"recipe.brioche.step.7.notes" = "‚Ä¢ Dough should rise until almost doubled\n‚Ä¢ Very important for softness";
"recipe.brioche.step.8.instruction" = "Egg Wash";
"recipe.brioche.step.8.notes" = "‚Ä¢ Mix 1 egg + a splash of milk\n‚Ä¢ Brush gently on top";
"recipe.brioche.step.9.instruction" = "Bake";
"recipe.brioche.step.9.notes" = "‚Ä¢ Preheat oven to 350¬∞F (175¬∞C)\n‚Ä¢ Bake 25‚Äì30 minutes (loaf)\n‚Ä¢ Or 18‚Äì22 minutes (individual balls)\n‚Ä¢ Golden brown on top";

// MARK: - Croissants
"recipe.croissants.step.1.instruction" = "Prepare the dough (D√©trempe)";
"recipe.croissants.step.1.notes" = "‚Ä¢ 500 g flour\n‚Ä¢ 10 g salt\n‚Ä¢ 60 g sugar\n‚Ä¢ 10 g dry yeast\n‚Ä¢ 300 g cold milk\n‚Ä¢ Mix until dough forms (no butter yet)\n‚Ä¢ Cover bowl";
"recipe.croissants.step.2.instruction" = "Light Kneading";
"recipe.croissants.step.2.notes" = "‚Ä¢ Knead gently until smooth (2‚Äì3 minutes)\n‚Ä¢ Do not overwork\n‚Ä¢ Shape into a rectangle\n‚Ä¢ Wrap and refrigerate";
"recipe.croissants.step.3.instruction" = "Prepare butter block";
"recipe.croissants.step.3.notes" = "‚Ä¢ 250 g cold butter\n‚Ä¢ Shape into flat square (about 15√ó15 cm)\n‚Ä¢ Wrap and refrigerate\n\n(Same duration as dough; they should be equally cold)";
"recipe.croissants.step.4.instruction" = "Encasing the butter";
"recipe.croissants.step.4.notes" = "‚Ä¢ Place butter block in center of dough\n‚Ä¢ Fold dough over butter (envelope shape)\n‚Ä¢ Seal edges tightly\n‚Ä¢ Refrigerate to relax gluten";
"recipe.croissants.step.5.instruction" = "Lamination ‚Äî Roll + First Fold (Single Fold)";
"recipe.croissants.step.5.notes" = "‚Ä¢ Roll dough into a long rectangle\n‚Ä¢ Fold in thirds (like a letter)\n‚Ä¢ Wrap and chill";
"recipe.croissants.step.6.instruction" = "Lamination ‚Äî Second Fold (Single Fold)";
"recipe.croissants.step.6.notes" = "‚Ä¢ Roll out again into long rectangle\n‚Ä¢ Fold in thirds again\n‚Ä¢ Wrap and chill";
"recipe.croissants.step.7.instruction" = "Lamination ‚Äî Third Fold (Final Fold)";
"recipe.croissants.step.7.notes" = "‚Ä¢ Roll and fold in thirds one last time\n‚Ä¢ This completes 3 folds\n‚Ä¢ Wrap and chill";
"recipe.croissants.step.8.instruction" = "Roll final sheet";
"recipe.croissants.step.8.notes" = "‚Ä¢ Roll dough to ~4 mm thickness\n‚Ä¢ Keep edges even\n‚Ä¢ Lightly flour as needed";
"recipe.croissants.step.9.instruction" = "Cut triangles";
"recipe.croissants.step.9.notes" = "‚Ä¢ Cut long isosceles triangles (base ~8‚Äì10 cm)\n‚Ä¢ Make small slit at base\n‚Ä¢ Stretch gently";
"recipe.croissants.step.10.instruction" = "Shape croissants";
"recipe.croissants.step.10.notes" = "‚Ä¢ Roll from base to tip\n‚Ä¢ Do not press too hard\n‚Ä¢ Place tip underneath on baking tray";
"recipe.croissants.step.11.instruction" = "Proofing (Final Rise)";
"recipe.croissants.step.11.notes" = "‚Ä¢ Let croissants rise at 77‚Äì80¬∞F (25‚Äì27¬∞C)\n‚Ä¢ Should become puffy, slightly jiggly\n‚Ä¢ Do NOT let butter melt";
"recipe.croissants.step.12.instruction" = "Egg wash";
"recipe.croissants.step.12.notes" = "‚Ä¢ Mix 1 egg + splash of milk\n‚Ä¢ Brush gently on top";
"recipe.croissants.step.13.instruction" = "Bake";
"recipe.croissants.step.13.notes" = "‚Ä¢ Preheat oven to 400¬∞F (200¬∞C)\n‚Ä¢ Bake 12 min, then lower to 375¬∞F (190¬∞C)\n‚Ä¢ Bake additional 8‚Äì10 minutes\n‚Ä¢ Golden, flaky, crispy layers";

