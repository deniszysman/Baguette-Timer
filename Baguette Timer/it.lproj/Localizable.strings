// MARK: - Brioche
"recipe.brioche.step.1.instruction" = "Preparare l'impasto (Mescolare #1)";
"recipe.brioche.step.1.notes" = "‚Ä¢ 250 g di latte (tiepido)\n‚Ä¢ 10 g di lievito secco\n‚Ä¢ 60 g di zucchero\n‚Ä¢ 3 uova\n‚Ä¢ Mescolare fino a combinare";
"recipe.brioche.step.2.instruction" = "Aggiungere farina e sale (Mescolare #2)";
"recipe.brioche.step.2.notes" = "‚Ä¢ Aggiungere 500 g di farina\n‚Ä¢ Aggiungere 10 g di sale\n‚Ä¢ Mescolare fino a quando l'impasto inizia a formarsi";
"recipe.brioche.step.3.instruction" = "Impastare con burro (Mescolare #3)";
"recipe.brioche.step.3.notes" = "‚Ä¢ Aggiungere 150 g di burro ammorbidito, pezzo per pezzo\n‚Ä¢ Impastare fino a quando l'impasto √® liscio, elastico e lucido\n‚Ä¢ L'impasto sar√† appiccicoso ‚Äî √® normale";
"recipe.brioche.step.4.instruction" = "Prima lievitazione (Fermentazione in massa)";
"recipe.brioche.step.4.notes" = "‚Ä¢ Coprire la ciotola\n‚Ä¢ Lasciare in una stanza calda\n‚Ä¢ L'impasto dovrebbe raddoppiare";
"recipe.brioche.step.5.instruction" = "Sgonfiare l'impasto";
"recipe.brioche.step.5.notes" = "‚Ä¢ Premere delicatamente per rimuovere l'aria in eccesso\n‚Ä¢ Non impastare troppo\n‚Ä¢ Preparare per la formatura";
"recipe.brioche.step.6.instruction" = "Formare la brioche";
"recipe.brioche.step.6.notes" = "Scegliere una opzione:\n‚Ä¢ Pagnotta di brioche ‚Üí dividere in 3‚Äì4 palline, posizionare nella teglia\n‚Ä¢ Palline di brioche ‚Üí formare palline rotonde compatte, posizionare nello stampo\n‚Ä¢ Coprire con canovaccio";
"recipe.brioche.step.7.instruction" = "Seconda lievitazione (Lievitazione finale)";
"recipe.brioche.step.7.notes" = "‚Ä¢ L'impasto dovrebbe lievitare fino a quasi raddoppiare\n‚Ä¢ Molto importante per la morbidezza";
"recipe.brioche.step.8.instruction" = "Spennellare con uovo";
"recipe.brioche.step.8.notes" = "‚Ä¢ Mescolare 1 uovo + un po' di latte\n‚Ä¢ Spennellare delicatamente sopra";
"recipe.brioche.step.9.instruction" = "Cuocere";
"recipe.brioche.step.9.notes" = "‚Ä¢ Preriscaldare il forno a 175¬∞C\n‚Ä¢ Cuocere 25‚Äì30 minuti (pagnotta)\n‚Ä¢ Oppure 18‚Äì22 minuti (palline singole)\n‚Ä¢ Dorato sopra";

// MARK: - Croissants
"recipe.croissants.step.1.instruction" = "Preparare l'impasto (D√©trempe)";
"recipe.croissants.step.1.notes" = "‚Ä¢ 500 g di farina\n‚Ä¢ 10 g di sale\n‚Ä¢ 60 g di zucchero\n‚Ä¢ 10 g di lievito secco\n‚Ä¢ 300 g di latte freddo\n‚Ä¢ Mescolare fino a formare l'impasto (ancora senza burro)\n‚Ä¢ Coprire la ciotola";
"recipe.croissants.step.2.instruction" = "Impasto leggero";
"recipe.croissants.step.2.notes" = "‚Ä¢ Impastare delicatamente fino a liscio (2‚Äì3 minuti)\n‚Ä¢ Non lavorare troppo\n‚Ä¢ Formare un rettangolo\n‚Ä¢ Avvolgere e refrigerare";
"recipe.croissants.step.3.instruction" = "Preparare il blocco di burro";
"recipe.croissants.step.3.notes" = "‚Ä¢ 250 g di burro freddo\n‚Ä¢ Formare un quadrato piatto (circa 15√ó15 cm)\n‚Ä¢ Avvolgere e refrigerare\n\n(Stessa durata dell'impasto; dovrebbero essere ugualmente freddi)";
"recipe.croissants.step.4.instruction" = "Avvolgere il burro";
"recipe.croissants.step.4.notes" = "‚Ä¢ Posizionare il blocco di burro al centro dell'impasto\n‚Ä¢ Piegare l'impasto sul burro (forma busta)\n‚Ä¢ Sigillare i bordi saldamente\n‚Ä¢ Refrigerare per rilassare il glutine";
"recipe.croissants.step.5.instruction" = "Laminazione ‚Äî Stendere + Prima piega (Piega singola)";
"recipe.croissants.step.5.notes" = "‚Ä¢ Stendere l'impasto in un rettangolo lungo\n‚Ä¢ Piegare in tre (come una lettera)\n‚Ä¢ Avvolgere e raffreddare";
"recipe.croissants.step.6.instruction" = "Laminazione ‚Äî Seconda piega (Piega singola)";
"recipe.croissants.step.6.notes" = "‚Ä¢ Stendere di nuovo in rettangolo lungo\n‚Ä¢ Piegare in tre di nuovo\n‚Ä¢ Avvolgere e raffreddare";
"recipe.croissants.step.7.instruction" = "Laminazione ‚Äî Terza piega (Piega finale)";
"recipe.croissants.step.7.notes" = "‚Ä¢ Stendere e piegare in tre un'ultima volta\n‚Ä¢ Questo completa 3 pieghe\n‚Ä¢ Avvolgere e raffreddare";
"recipe.croissants.step.8.instruction" = "Stendere il foglio finale";
"recipe.croissants.step.8.notes" = "‚Ä¢ Stendere l'impasto a ~4 mm di spessore\n‚Ä¢ Mantenere i bordi uniformi\n‚Ä¢ Infarinare leggermente se necessario";
"recipe.croissants.step.9.instruction" = "Tagliare triangoli";
"recipe.croissants.step.9.notes" = "‚Ä¢ Tagliare triangoli isosceli lunghi (base ~8‚Äì10 cm)\n‚Ä¢ Fare un piccolo taglio alla base\n‚Ä¢ Stirare delicatamente";
"recipe.croissants.step.10.instruction" = "Formare i croissant";
"recipe.croissants.step.10.notes" = "‚Ä¢ Arrotolare dalla base alla punta\n‚Ä¢ Non premere troppo forte\n‚Ä¢ Posizionare la punta sotto sulla teglia";
"recipe.croissants.step.11.instruction" = "Lievitazione (Lievitazione finale)";
"recipe.croissants.step.11.notes" = "‚Ä¢ Lasciare lievitare i croissant a 25‚Äì27¬∞C\n‚Ä¢ Dovrebbero diventare soffici, leggermente traballanti\n‚Ä¢ NON lasciare sciogliere il burro";
"recipe.croissants.step.12.instruction" = "Spennellare con uovo";
"recipe.croissants.step.12.notes" = "‚Ä¢ Mescolare 1 uovo + un po' di latte\n‚Ä¢ Spennellare delicatamente sopra";
"recipe.croissants.step.13.instruction" = "Cuocere";
"recipe.croissants.step.13.notes" = "‚Ä¢ Preriscaldare il forno a 200¬∞C\n‚Ä¢ Cuocere 12 min, poi abbassare a 190¬∞C\n‚Ä¢ Cuocere altri 8‚Äì10 minuti\n‚Ä¢ Strati dorati, sfogliati, croccanti";

